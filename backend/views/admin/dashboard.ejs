<script>
    // Bar Chart - Residents & Security Guards Count
    const barCtx = document.getElementById('barChart').getContext('2d');
    new Chart(barCtx, {
        type: 'bar',
        data: {
            labels: ['Residents', 'Security Guards'],
            datasets: [{
                label: 'Count',
                data: [250, 15],
                backgroundColor: ['#001F3F', '#FF6600'],
                borderColor: ['#004080', '#FF4500'],
                borderWidth: 2
            }]
        },
        options: {
            responsive: true,
            scales: {
                y: { beginAtZero: true }
            }
        }
    });

    // Line Chart - Monthly Registrations
    const lineCtx = document.getElementById('lineChart').getContext('2d');
    new Chart(lineCtx, {
        type: 'line',
        data: {
            labels: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun'],
            datasets: [{
                label: 'Residents Registered',
                data: [20, 35, 40, 50, 55, 65],
                borderColor: '#001F3F',
                backgroundColor: 'rgba(0, 31, 63, 0.2)',
                borderWidth: 2
            }, {
                label: 'Guards Registered',
                data: [2, 5, 8, 10, 12, 15],
                borderColor: '#FF6600',
                backgroundColor: 'rgba(255, 102, 0, 0.2)',
                borderWidth: 2
            }]
        },
        options: {
            responsive: true,
            scales: {
                y: { beginAtZero: true }
            }
        }
    });
</script>


