<script>



    async function register()
    {

        if($("#vehicleForm").valid()) {
            var email = document.getElementById("email").value;
            var oname = document.getElementById("oname").value;
            var type = document.getElementById("type").value;
            var vnumber = document.getElementById("vnumber").value;
            var rc = document.getElementById("rc").files[0];
            // console.log(firstname,lastname,city,photo);

            var formData = new FormData();
            formData.append('email',email)
            formData.append('oname',oname)
            formData.append('type',type)
            formData.append('vnumber',vnumber)
            formData.append('rc',rc)
            var res =await fetch("/user/regsvehical",{
                method:"POST",
                body:formData
            });
            res = await res.json();
            console.log(res);
            if(res.error){
                Qual.errordb("Error", res.message);
            }else
            {
                Qual.successdb("Success", res.message);

            }
        }
    }




    async function fetchbookeddata() {
        try {
            // console.log(service)
            var res = await fetch('/user/fetchvehical');
            res = await res.json();
            var records = res.records;
            console.log(records);

            var html = ``;
            for (var x of records) {

                html += `<tr>`
                html += `<td><div class=" col-md-4 mb-3" >

<div class="card">
    <div class="card-body">
        <h5 class="card-title">Vehical Details</h5>
        <p><strong>Email:</strong> <span id="email">${x.email}</span></p>
        <p><strong>Owner Name:</strong> <span id="gate">${x.oname}</span></p>
        <p><strong>Vehical Type:</strong> <span id="shift">${x.type}</span></p>
        <p><strong>Vehical Number:</strong> <span id="shift">${x.vnumber}</span></p>
        <p><strong>Status:</strong> <span id="shift" class="text-success">${x.status}</span></p>
  <p><strong>Rc Photo:</strong> <span id="shift" class="text-success"><img src="${x.rc}" height="70px "></span></p>


                </div>
</div></div></td>`

                html += `</tr>`

            }
            document.getElementById('cards2').innerHTML = html;
        } catch (e) {
            console.log(e.message);
        }
    }





</script>





